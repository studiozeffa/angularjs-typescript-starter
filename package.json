{
  "name": "angularjs-typescript-starter",
  "version": "1.0.0",
  "description": "A starter repo for an AngularJS app, using TypeScript.",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config jest-unit.config.js --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:e2e": "jest --config jest-e2e.config.js"
  },
  "keywords": ["angularjs", "typescript"],
  "author": "Tom Spencer <tom.spencer@studiozeffa.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/angular": "^1.6.47",
    "@types/expect-puppeteer": "^2.2.2",
    "@types/jest": "^23.1.1",
    "@types/jest-environment-puppeteer": "^2.2.0",
    "@types/puppeteer": "^1.3.4",
    "angular": "^1.7.2",
    "autoprefixer": "^8.6.3",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jest-environment-node": "^23.2.0",
    "jest-puppeteer": "^3.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "mkdirp": "^0.5.1",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "puppeteer": "^1.5.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
